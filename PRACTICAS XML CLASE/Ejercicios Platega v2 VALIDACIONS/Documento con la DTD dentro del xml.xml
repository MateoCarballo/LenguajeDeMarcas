<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE tema [
 <!ELEMENT tema (autor, apartado+)>
 <!ATTLIST tema
                        titulo CDATA #REQUIRED
                        unidade CDATA #REQUIRED>
 <!ELEMENT autor (#PCDATA)>
 <!ELEMENT apartado (#PCDATA)>
 <!ATTLIST apartado
                        numero CDATA #REQUIRED>
]>

